<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>


    <table border="1">

        <tr>

            <th>FirstName</th>
            <th>LastName</th>
            <th>Birth Date</th>
            <th>Mobile</th>
            <th>Action</th>

        </tr>

        <% for(var i=0; i<users.length;i++){ %>
            <tr>


                <td>
                    <%= users[i].first_name %>
                </td>
                <td>
                    <%= users[i].last_name %>
                </td>
                <td>
                    <%= users[i].birth_date %>
                </td>
                <td>
                    <%= users[i].user_mobile %>
                </td>

                <td>
                    <a href='edit/<%= users[i]._id %>'>Edit</a> |

                    <a href='delete/<%= users[i]._id %>'>Delete</a>
                </td>

            </tr>
            <% } %>

    </table>
</body>

</html>